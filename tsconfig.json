{
  "compilerOptions": {
    "preserveConstEnums": true,
    "noEmit": true,
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "es2022",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node", "jest"]
  }
}
